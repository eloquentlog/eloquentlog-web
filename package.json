    "rollup-plugin-terser": "^7.0.1",
    "rollup-plugin-typescript2": "0.27.2",
    "stylint": "2.0.0",
    "stylus": "0.54.8",
    "styr": "0.0.32",
    "ts-jest": "^26.5.5",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.9.5",
    "yargs-parser": ">=5.0.0-serucity.0"
  }
