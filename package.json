{
  "name": "eloquentlog-web",
  "descrpiton": "The web application of Eloquentlog.",
  "version": "0.0.17",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/eloquentlog/eloquentlog-web"
  },
  "scripts": {
    "build:release": "rollup -c --configBuildRelease",
    "build:debug": "rollup -c --configBuildDebug",
    "check": "tsc --project tsconfig.json --noEmit --pretty",
    "lint:styl": "stylint -c .stylintrc src",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --no-cache",
    "coverage": "jest --no-cache --coverage",
    "watch:build": "rollup -w -c --configBuildDebug",
    "watch:server": "rollup -w -c --configRunServer"
  },
  "devDependencies": {
    "@primer/octicons": "^17.0.0",
    "@rollup/plugin-buble": "0.21.3",
    "@rollup/plugin-commonjs": "23.0.7",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "14.0.1",
    "@rollup/plugin-replace": "4.0.0",
    "@types/history": "4.7.11",
    "@types/jest": "^27.0.0",
    "@types/js-cookie": "3.0.0",
    "axios": "0.27.2",
    "history": "4.10.1",
    "identity-obj-proxy": "3.0.0",
    "inferno": "^7.4.3",
    "inferno-hyperscript": "^7.4.8",
    "inferno-router": "^7.4.8",
    "inferno-test-utils": "^8.0.0",
    "jest": "^27.3.1",
    "js-cookie": "2.2.1",
    "magic-string": "0.26.3",
    "mkdirp": "^1.0.4",
    "rollup": "2.79.0",
