{
  "name": "eloquentlog-web",
  "descrpiton": "The web application of Eloquentlog.",
  "version": "0.0.15",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/eloquentlog/eloquentlog-web"
  },
  "scripts": {
    "build:production": "rollup -c --configBuildProduction",
    "build:development": "rollup -c --configBuildDevelopment",
    "check": "tsc --project tsconfig.json --noEmit --pretty",
    "lint:styl": "stylint -c .stylintrc src",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --no-cache",
    "coverage": "jest --no-cache --coverage",
    "watch:build": "rollup -w -c --configBuildDevelopment",
    "watch:server": "rollup -w -c --configRunServer"
  },
  "devDependencies": {
    "@primer/octicons": "^16.0.0",
    "@rollup/plugin-buble": "0.21.3",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-replace": "3.0.0",
    "@types/history": "4.7.9",
    "@types/jest": "^27.0.0",
    "@types/js-cookie": "3.0.0",
    "axios": "0.24.0",
    "history": "4.10.1",
    "identity-obj-proxy": "3.0.0",
    "inferno": "^7.4.3",
    "inferno-hyperscript": "^7.4.8",
    "inferno-router": "^7.4.8",
    "inferno-test-utils": "^7.4.8",
    "jest": "^27.3.1",
    "js-cookie": "2.2.1",
    "magic-string": "0.25.7",
    "mkdirp": "^1.0.4",
    "rollup": "2.58.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-serve": "1.1.0",
    "rollup-plugin-strip": "1.2.2",
    "rollup-plugin-stylus-compiler": "1.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "stylint": "^1.3.8",
    "stylus": "0.55.0",
    "styr": "0.0.32",
    "ts-jest": "^27.0.7",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.4.4",
    "yargs-parser": ">=5.0.0-serucity.0"
  }
}
