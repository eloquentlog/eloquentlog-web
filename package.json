{
  "name": "eloquentlog-web-console",
  "descrpiton": "The console web application of Eloquentlog.",
  "version": "0.0.12",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/eloquentlog/eloquentlog-web-console"
  },
  "scripts": {
    "build:production": "rollup -c --configBuildProduction",
    "build:development": "rollup -c --configBuildDevelopment",
    "lint:styl": "stylint -c .stylintrc src",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json",
    "test": "rollup -c --configBuildTest && jest --no-cache",
    "watch:build": "rollup -w -c --configBuildDevelopment",
    "watch:server": "rollup -w -c --configRunServer"
  },
  "devDependencies": {
    "@primer/octicons": "^13.0.0",
    "@rollup/plugin-buble": "0.21.3",
