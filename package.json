    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-replace": "2.4.2",
    "@types/history": "4.7.8",
    "@types/jest": "^26.0.10",
    "@types/js-cookie": "2.2.6",
    "axios": "0.21.1",
    "history": "4.10.1",
    "inferno": "^7.4.3",
    "inferno-hyperscript": "^7.4.3",
    "inferno-router": "^7.4.8",
    "inferno-test-utils": "^7.4.8",
    "jest": "^26.6.3",
    "js-cookie": "2.2.1",
    "magic-string": "0.25.7",
    "mkdirp": "^1.0.4",
    "rollup": "2.47.0",
