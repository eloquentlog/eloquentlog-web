{
  "compilerOptions": {
    "allowJs": true
  , "checkJs": true
  , "alwaysStrict": true
  , "allowSyntheticDefaultImports": true
  , "esModuleInterop": false
  , "baseUrl": "."
  , "module": "esnext"
  , "moduleResolution": "node"
  , "noEmit": true
  , "noImplicitAny": true
  , "noUnusedLocals": true
  , "noUnusedParameters": true
  , "preserveConstEnums": true
  , "removeComments": true
  , "resolveJsonModule": true
  , "skipLibCheck": true
  , "sourceMap": true
  , "strict": false
  , "target": "ES2019"
  , "lib": [
      "dom"
    , "ES2019"
    ]
  , "typeRoots": [
      "./node_modules/@types"
    ]
  }
  , "exclude": [
    "config.json"
  , "rollup.config.js"
  , "jest.config.js"
  , "dst/**/*"
  ]
}
